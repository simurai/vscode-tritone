		"type": "git",
		"url": "https://github.com/simurai/vscode-tritone"
	},
	"bugs": {
		"url": "https://github.com/simurai/vscode-tritone/issues"
	},
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Themes"
	],
